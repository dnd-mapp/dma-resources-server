// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema
generator client {
    provider   = "prisma-client"
    output     = "../src/prisma-client"
    engineType = "client"
}

datasource db {
    provider          = "mysql"
    url               = env("MARIADB_URL")
    shadowDatabaseUrl = env("MARIADB_SHADOW_URL")
}
